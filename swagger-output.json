{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "Change Password Demo",
    "description": ""
  },
  "servers": [
    {
      "url": "http://localhost:3000/"
    }
  ],
  "paths": {},
  "components": {
    "schemas": {
      "email": {
        "type": "string",
        "example": "jw80301@gmail.com"
      },
      "changePasswordReq": {
        "type": "object",
        "properties": {
          "email": {
            "$ref": "#/components/schemas/email"
          },
          "password": {
            "type": "string",
            "example": "abc123"
          },
          "newPassword": {
            "type": "string",
            "example": "bb"
          }
        }
      },
      "emailOTPReq": {
        "type": "object",
        "properties": {
          "email": {
            "$ref": "#/components/schemas/email"
          }
        }
      },
      "verifyOTPReq": {
        "type": "object",
        "properties": {
          "verification_key": {
            "type": "string",
            "example": ""
          },
          "otp": {
            "type": "string",
            "example": ""
          },
          "email": {
            "$ref": "#/components/schemas/email"
          }
        }
      }
    },
    "securitySchemes": {
      "apiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "authorization",
        "description": "any description..."
      }
    }
  }
}